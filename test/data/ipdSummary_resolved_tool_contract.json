{
  "_comment": "Example resolved tool contract for running ipdSummary.py",
  "tool_contract": {
    "tool_contract_id": "kineticsTools.ipdSummary",
    "task_type": "pbsmrtpipe.constants.distributed_task",
    "input_files": [
      "/mnt/secondary-siv/testdata/kineticsTools/Hpyl_1_5000.xml",
      "/mnt/secondary-siv/references/Helicobacter_pylori_J99/sequence/Helicobacter_pylori_J99.fasta"
    ],
    "output_files": [
      "tst_basemods_tool_contract.gff",
      "tst_basemods_tool_contract.csv"
