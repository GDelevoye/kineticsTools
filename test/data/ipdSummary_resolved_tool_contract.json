    "options": {
      "basemods.identify": "m6A,m4C",
      "basemods.max_length": 3000000000,
      "basemods.compute_methyl_fraction": false,
      "basemods.pvalue": 0.001
    },
    "nproc": 12,
    "resources": []
  },
  "driver": {
    "exe": "ipySummary.py --resolved-tool-contract",
    "env": {}
  }
}
